<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Portal.Modules.OrientalSails" namespace="Portal.Modules.OrientalSails.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="QRoomType" table="os_QRoomType" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="false" />
    </property>
    <many-to-one name="Group" column="GroupCruiseId" class="QCruiseGroup"/>
  </class>
</hibernate-mapping>