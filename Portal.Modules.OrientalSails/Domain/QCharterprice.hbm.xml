<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Portal.Modules.OrientalSails" namespace="Portal.Modules.OrientalSails.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="QCharterPrice" table="q_CharterPrice" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>

    <many-to-one name="QQuotation" column="QuotationId" class="QQuotation"/>

    <many-to-one name="GroupCruise" column="GroupCruiseId" class="QCruiseGroup"/>
    
    <property name="AgentLevelCode"/>
    <property name="Trip"/>
    <many-to-one name="Cruise" column="CruiseId" class="Cruise"/>

    <property name="Validname">
      <column name="ValidName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Validfrom">
      <column name="ValidFrom" sql-type="int" not-null="false" />
    </property>
    <property name="Validto">
      <column name="ValidTo" sql-type="int" not-null="false" />
    </property>
    <property name="Priceusd">
      <column name="PriceUsd" sql-type="money" not-null="false" />
    </property>
    <property name="Pricevnd">
      <column name="PriceVnd" sql-type="money" not-null="false" />
    </property>
  </class>
</hibernate-mapping>